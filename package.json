{
  "name": "hubot-ibmcloud-deploy",
  "version": "0.0.19",
  "description": "GitHub Project Deployment to IBM Cloud for hubot",
  "main": "index.js",
  "repository": {
    "url": "https://github.com/ibm-cloud-solutions/hubot-ibmcloud-deploy",
    "type": "git"
  },
  "scripts": {
    "start": "source config/env && hubot -a shell",
    "slack": "source config/env && hubot -a slack",
    "postinstall": "initDb src/nlc/NLC.json",
    "facebook": "source config/env && hubot -a fb",
    "lint": "eslint src/**/*.js test/*.js index.js",
    "test": ". test/.env && mocha test",
    "coverage": ". test/.env && istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage"
  },
  "keywords": [
    "hubot",
    "hubot-scripts",
    "bluemix",
    "cloud",
    "slack"
  ],
  "author": "ibm-cloud-solutions",
  "license": "Apache-2.0",
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-script": "^1.10.0",
    "coveralls": "^2.11.12",
    "eslint": "^2.9.0",
    "eslint-config-strongloop": "^2.0.1",
    "hubot-fb": "^3.0.0",
    "hubot-help": "^0.2.0",
    "hubot-ibmcloud-auth": ">=0.0.8",
    "hubot-ibmcloud-formatter": ">=0.0.10",
    "hubot-ibmcloud-nlc": ">=0.0.20",
    "hubot-shell": "^1.0.2",
    "hubot-slack": "^4.0.2",
    "hubot-test-helper": "^1.4.4",
    "istanbul": "0.4.0",
    "mocha": "^2.4.5",
    "nock": "^8.0.0"
  },
  "peerDependencies": {
    "hubot": "^2.19.0"
  },
  "engines": {
    "node": ">= 4.2.0"
  },
  "files": [
    "src",
    "index.js"
  ],
  "dependencies": {
    "adm-zip": "^0.4.7",
    "hubot-cf-convenience": "latest",
    "hubot-conversation": "^1.1.1",
    "hubot-ibmcloud-activity-emitter": "latest",
    "hubot-ibmcloud-cognitive-lib": "latest",
    "hubot-ibmcloud-utils": "latest",
    "i18n-2": "*",
    "lodash": "^4.13.1",
    "request": "^2.72.0",
    "yamljs": "^0.2.8"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
